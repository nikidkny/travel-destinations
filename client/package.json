    "types": "tsc -p tsconfig.json --noEmit"
  },
  "devDependencies": {
    "@packages/types": "*",
    "autoprefixer": "^10.4.20",
    "postcss": "^8.4.47",
    "tailwindcss": "^3.4.13",
    "typescript": "^5.6.2",
    "vite": "^5.4.8"
  },
  "dependencies": {
    "axios": "^1.7.7"
  }
}
